{
  "name": "fitpal-backend",
  "version": "0.0.1",
  "main": "./src/index.ts",
  "repository": "git@github.com:AMaklakov/fitpal-backend.git",
  "scripts": {
    "build": "rm -rf dist && ttsc --sourceMap -p tsconfig.json",
    "prod": "NODE_ENV=production node dist/index.js",
    "prestart": "yarn",
    "start": "NODE_ENV=developement nodemon"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "axios-mock-adapter": "^1.18.1",
    "big.js": "^5.2.2",
    "dotenv": "^8.2.0",
    "dotenv-flow": "^3.1.0",
    "fastify": "^2.13.1",
    "fastify-blipp": "^2.3.0",
    "fastify-compress": "^2.0.1",
    "fastify-cors": "^3.0.3",
    "fastify-helmet": "^3.0.2",
    "fastify-mongodb": "^2.0.0",
    "fastify-plugin": "^1.6.1",
    "fastify-static": "^2.7.0",
    "fastify-swagger": "^2.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.9.10",
    "nanoid": "^3.1.3",
    "source-map-support": "^0.5.18"
  },
  "devDependencies": {
    "@types/big.js": "^4.0.5",
    "@types/dotenv-flow": "^3.0.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.2",
    "@types/mongoose": "^5.7.14",
    "@types/source-map-support": "^0.5.1",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "lint-staged": "^10.1.7",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.9.0",
    "tsconfig-paths": "^3.9.0",
    "ttypescript": "^1.5.10",
    "typescript": "^3.8.3",
    "typescript-transform-paths": "^1.1.14"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json}": [
      "prettier --write"
    ],
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write",
      "jest --bail --findRelatedTests"
    ],
    "src/i18n/messages/*.json": [
      "node scripts/compare-json-file-keys.js",
      "node scripts/sort-json-file.js",
      "prettier --write"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "node --inspect=0.0.0.0:9228 -r tsconfig-paths/register -r ts-node/register ./src/index.ts",
    "ext": "ts, js"
  }
}
